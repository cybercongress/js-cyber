{
  "name": "@cybercongress/cyber-js",
  "version": "0.2.4",
  "description": "Cyber SDK",
  "license": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "*.md",
    "!*.spec.*",
    "!**/testdata/"
  ],
  "repository": {
    
    "type": "git",
    "url": "https://github.com/cybercongress/cyber-js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "get-proto": "./get-proto.sh",
    "define-proto": "./define-proto.sh",
    "postdefine-proto": "prettier --write \"src/codec/**/*.ts\"",
    "lint": "eslint --max-warnings 0 \"./**/*.ts\"",
    "lint-fix": "eslint --fix --max-warnings 0 \"./**/*.ts\"",
    "format": "prettier --write --loglevel warn \"./src/**/*.ts\"",
    "format-text": "prettier --write \"./*.md\""
  },
  "dependencies": {
    "@cosmjs/amino": "v0.26.4",
    "@cosmjs/cosmwasm-launchpad": "v0.25.6",
    "@cosmjs/cosmwasm-stargate": "v0.26.4",
    "@cosmjs/crypto": "v0.26.4",
    "@cosmjs/math": "v0.26.4",
    "@cosmjs/proto-signing": "v0.26.4",
    "@cosmjs/stargate": "v0.26.4",
    "@cosmjs/tendermint-rpc": "v0.26.4",
    "@cosmjs/utils": "v0.26.4",
    "cosmjs-types": "^0.2.0",
    "long": "^4.0.0",
    "pako": "^2.0.3",
    "process": "^0.11.10",
    "protobufjs": "~6.10.2",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@cosmjs/encoding": "^0.26.0",
    "@types/long": "^4.0.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/pako": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.5",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.2",
    "ts-proto": "^1.83.1",
    "typescript": "^4.3.5"
  }
}
